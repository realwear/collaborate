  "scripts": {
    "ai:local": "yarn dev:generateenv && AZ_USE_CLI=true env-cmd --silent -f .localConfigs nx run-many -t serve -p aibackend ai loader",
    "ai:deploy_loader": "nx build loader --configuration production && cp -r dist/apps/loader/browser/* android/apps/ai/app/src/main/assets/",
    "storybook": "nx run uxlib:storybook",
    "dev:generateenv": "env-cmd --silent -f .localConfigs node .devops/generate-environment-file.js",
    "lint": "nx run-many --target lint --all",
    "prettier": "prettier --check \"{apps,libs}/**/*.{ts,js,json,html,css,scss,md}\"",
    "build": "nx run-many --target build --all",
    "test": "nx run-many -t test -p uxlib auth rwtdevice rxjs speech --coverage",
    "rwt:debugger": "env-cmd --silent -f .localConfigs nx run-many -t serve -p rwtdebugger rwtanontoken",
    "rwt:deploy_loader": "nx build loader --configuration production && cp -r dist/apps/loader/browser/* android/apps/rwt/app/src/main/assets/",
    "rwt:local": "yarn dev:generateenv && env-cmd --silent -f .localConfigs nx run-many -t serve -p rwtdevice rwtteamsaddon rwtbackend",
    "rwt:local:backend": "env-cmd --silent -f .localConfigs nx serve rwtbackend",
    "sample:custom-dashboard": "yarn dev:generateenv && env-cmd --silent -f .localConfigs nx run-many -t serve -p sample-custom-dashboard rwtbackend",
    "gpl:header": "node .devops/add-header.js"
  },
